   "source": [
    "def simple_scaled_circle(x: float, canvas: mg.Canvas) -> None:\n",
    "  canvas.circle(canvas.center, mg.lerp(x, 0.01, canvas.ysize/2), color='navy', style='stroke', width='20p')\n",
    "\n",
    "mg.show(simple_scaled_circle)"
   ]
  }
